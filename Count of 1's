import java.io.*;
import java.util.*;
public class CandidateCode {
    public static void main(String args[] ) throws Exception {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int count = 0;
        for(int i = 1;i<=n;i++)
        {
            int a = sc.nextInt();
            if (a == 1)
                count++;
        }
        System.out.println(count);
   }
}
